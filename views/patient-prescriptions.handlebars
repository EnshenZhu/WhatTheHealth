

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>WhatTheHealth</title>
    <link rel="stylesheet" href="https://cdn.staticfile.org/foundation/5.5.3/css/foundation.min.css">
    <script src="https://cdn.staticfile.org/jquery/2.1.1/jquery.min.js"></script>
    <script src="https://cdn.staticfile.org/foundation/5.5.3/js/foundation.min.js"></script>
    <script src="https://cdn.staticfile.org/foundation/5.5.3/js/vendor/modernizr.js"></script>
</head>

  <body>

    
    <div class="row">

      <nav class="top-bar" data-topbar>

        <ul class="title-area">
          <ol class="name">
            <h1><a href="#">WhatTheHealth</a></h1>
          </ol>          
        </ul>

        <section class="top-bar-section">
          <ul class="right">
            <li class="active"><a href="#">Sign out</a></li>
          </ul>
        </section>
      </nav>
            
    </div> 

    <div class="row">

      <div class="medium-4 columns" style="background-color:#f1f1f1;">
        <ul class="side-nav">
          <li class="active"><a href="#">Calendar</a></li>
          <li><a href="#">Prescription</a></li>
          <li><a href="#">Book Appointment</a></li>
          <li><a href="#">Dashboard</a></li>
          <li><a href="#">My Profile</a></li>   
        </ul>
      </div>      

      <div class="medium-4 columns" style="background-color:#f1f1f1;">
        <h1>Patient Prescriptions</h1>
        <table class="patient-prescriptions .table">
          <tr>
            <th scope="col">Drug Name</th>
            <th scope="col">Form</th>
            <th scope="col">Dosage</th>
            <th scope="col">Quantity</th>
            <th scope="col">Refills</th>
            <th scope="col">Expiration</th>
            <th scope="col">Instructions</th>
            <th scope="col">Doctor</th>
            <th scope="col">Pharmacy</th>
            <th scope="col">Pharmacy Phone</th>
            <th scope="col">Pharmacy Fax</th>
            <th scope="col">Status</th>
          </tr>
        {{#each prescriptions}}
          <tr>
            <td>{{this.drug}}</td>
            <td>{{this.form}}</td>
            <td>{{this.dosage}}</td>
            <td>{{this.quantity}}</td>
            <td>{{this.refills}}</td>
            <td>{{this.expiration}}</td>
            <td>{{this.instructions}}</td>
            <td>{{this.doctorName}}</td>
            <td>{{this.pharmacy}}</td>
            <td>{{this.pharmacyPhone}}</td>
            <td>{{this.pharmacyFax}}</td>
            <td>{{this.status}}</td>
          </tr>
        <tr>

        {{/each}}
        </table>
      </div>      

    </div>
  
  </body>
</html>
